<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/pc192"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pc192_round"
        android:supportsRtl="true"
        android:configChanges="locale|layoutDirection"
        android:theme="@style/Theme.CaritasRig"
        android:screenOrientation="portrait"

        tools:targetApi="31">
        

        <activity
            android:name=".activity.CheckActivity"
            android:noHistory="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.homepage.home.HomeActivity" />
        <activity android:name="com.canhub.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat"/>
        <activity android:name=".activity.homepage.profileicon.AboutUsActivity" />
        <activity android:name=".activity.homepage.profileicon.SettingsActivity"/>
        <activity android:name=".activity.homepage.profileicon.profilesettings.ProfileSettingsActivity"/>
    </application>

</manifest>